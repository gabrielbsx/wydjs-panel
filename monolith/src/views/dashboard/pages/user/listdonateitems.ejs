<div class="col-md-12 grid-margin stretch-card list-donate-items">
    <div class="card">
        <div class="card-body">
            <div class="d-flex flex-row justify-content-between border-bottom">
                Lista de bonificações
            </div>
            <div class="d-flex table-responsive flex-row row px-4 justify-content-center my-2 py-4 text-center list-donate-items-table">
                <table class="table table-dark table-hover table-striped">
                    <thead>
                        <tr>
                            <th>Nome do item</th>
                            <th>Item Index</th>
                            <th>Effect 01</th>
                            <th>Effect Value 01</th>
                            <th>Effect 02</th>
                            <th>Effect Value 02</th>
                            <th>Effect 03</th>
                            <th>Effect Value 03</th>
                            <th colspan="2">Ação</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data.forEach((value, key) => { %>
                            <tr>
                                <td><%= value.itemname %></td>
                                <td><%= value.item_id %></td>
                                <td><%= value.eff1 %></td>
                                <td><%= value.effv1 %></td>
                                <td><%= value.eff2 %></td>
                                <td><%= value.effv2 %></td>
                                <td><%= value.eff3 %></td>
                                <td><%= value.effv3 %></td>
                                <td><a class="btn btn-primary" href="/update-donate-items?id=<%= value.id %>">Atualizar</a></td>
                                <td><a class="btn btn-danger" href="/delete-donate-items?id=<%= value.id %>">Deletar</a></td>
                            </tr>
                        <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>