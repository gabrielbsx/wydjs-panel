    grecaptcha.ready(function () {
        grecaptcha.execute("<%= recaptcha_site %>", {
            action: "validate_recaptcha"
        }).then(function (token) {
            if ($(".g-recaptcha-response").length) {
                $.each($(".g-recaptcha-response"), (index, value) => {
                    value.value = token;
                });
            }
        });
    });
</script>
<% if (typeof user === 'undefined') { %>
    <script>

        async function setlogin() {
            await unsetregister();
            await unsetrecovery();
            $("#forms > #login").show();
        }

        async function setregister() {
            await unsetlogin();
            await unsetrecovery();
            $("#forms > #register").show();
        }

        async function setrecovery() {
            await unsetlogin();
            await unsetregister();
            $("#forms > #recovery").show();
        }

        async function unsetrecovery() {
            $("#forms > #recovery").hide();
        }
        
        async function unsetlogin() {
            $("#forms > #login").hide();
        }
        
        async function unsetregister() {
            $("#forms > #register").hide();
        }

        $(document).ready(async function () {
            await setlogin();
        });

    </script>
<% } %>
